class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        note={}
        #notice the range wrting norm
        for i in range(len(nums)):
            now_num=nums[i]
            needed=target-now_num
            if needed in note: 
                #找到了
                return i,note[needed]
            else:
                #要找的不在等待区
                note[nums[i]]=i





